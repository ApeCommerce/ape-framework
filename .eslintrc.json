        "**/*.js",
        "src/node/**/*.ts",
        "src/utils/**/*.ts",
        "test/unit/node/**/*.ts",
        "test/unit/utils/**/*.ts"
      ],
      "extends": [
        "@apeframework"
      ],
      "rules": {},
      "overrides": [
        {
          "files": [
            "**/*.ts"
          ],
          "plugins": [
            "@typescript-eslint"
          ],
          "rules": {},
          "overrides": [
            {
              "files": [
                "**/*.spec.ts"
              ],
              "plugins": [
                "jest"
              ],
              "rules": {}
            }
          ]
        }
      ]
    }
  ]
}