{
  "overrides": [
    {
      "files": [
        "**/*.js",
        "src/env/**/*.ts",
        "src/node/**/*.ts",
        "src/utils/**/*.ts",
        "test/unit/env/**/*.ts",
        "test/unit/node/**/*.ts",
        "test/unit/utils/**/*.ts"
      ],
      "extends": [
        "@apeframework"
      ],
      "plugins": [
        "@stylistic",
        "import"
      ],
      "rules": {
        "no-restricted-exports": [
          "error",
          {
            "restrictDefaultExports": {
              "direct": true
            }
          }
        ],
        "@stylistic/array-bracket-newline": [
          "error",
          {
            "multiline": true
          }
        ],
        "import/dynamic-import-chunkname": [
          "off"
        ],
        "import/no-default-export": [
          "error"
        ],
        "import/prefer-default-export": [
          "off"
        ]
      },
      "overrides": [
        {
          "files": [
            "**/*.ts"
          ],
          "plugins": [
            "@typescript-eslint"
          ],
          "rules": {},
          "overrides": [
            {
              "files": [
                "**/*.spec.ts"
              ],
              "plugins": [
                "jest"
              ],
              "rules": {
                "jest/expect-expect": [
                  "off"
                ]
              }
            }
          ]
        }
      ]
    }
  ]
}